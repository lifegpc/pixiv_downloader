{
    "name": "all features",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "extensions": [
        "rust-lang.rust-analyzer"
    ],
    "settings": {
        "rust-analyzer.cargo.features": [
            "all"
        ]
    },
    "customizations": {
        "codespaces": {
            "repositories": {
                "lifegpc/c-utils": {
                    "permissions": {
                        "contents": "write",
                        "workflows": "write"
                    }
                }
            }
        }
    },
    "postCreateCommand": "cd /workspaces/pixiv_downloader && git submodule update --init"
}
